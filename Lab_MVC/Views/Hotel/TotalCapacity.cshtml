@model List<Room>

    @if (@ViewBag.totalCapacity <= 0)
    {
        <h2>“No more than 25 employees should be in this building, according to local fire codes.”</h2>
    }
    else {
        <h2>Total number of rooms remaining @ViewBag.totalCapacity</h2>
    }


    @foreach(Room r in Model){
        <h4>Room number: @r.Number</h4>
        <h4>Capacity: @r.Capacity</h4>
        
    }